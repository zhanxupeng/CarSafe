<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
 <hibernate-mapping>
 	<class name="com.mr.timecar.Timecar" table="timecar">
 		<!-- idå€¼ -->
 		<id name="id" column="id" type="int">
			<generator class="native"/>
		</id>
		<property name="car_id" type="int">
			<column name="car_id"></column>
		</property>
		<property name="xx" type="double">
			<column name="xx"></column>
		</property>
		<property name="yy" type="double">
			<column name="yy"></column>
		</property>
		<property name="speed" type="double">
			<column name="speed"></column>
		</property>
		<property name="time" type="int">
			<column name="time"></column>
		</property>
 	</class>
 </hibernate-mapping>